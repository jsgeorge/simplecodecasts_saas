<h1 class="center-text">User Community </h1>
<div class="row">
   <div class="col-md-8 col-md-offset"> 
        <ul class="list-unstyled">
            <% @users.each do |user| %>
                <% if user.profile %>
                  <li>
                      <div class="well row <%= cycle('white-bg', '') %>">
                          <div class="col-sm-4 text-center">
                              <%# avatar %>
                          </div>
                          <div class="col-md-6">
                              <%= link_to user do %>
                              <h2><%= user.profile.first_name %>&nbsp;<%= user.profile.last_name %></h2><br/> <% end %>
                              <p><%= user.profile.job_title %></p>
                            
                            </div>
                        </div>
                  </li>
                <%end %>
            <%end %>
        </ul>
    </div>
</div>